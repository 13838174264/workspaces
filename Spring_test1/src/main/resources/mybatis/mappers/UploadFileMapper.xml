<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jk.mapper.UploadFileMapper">
	<insert id="uploadFile" parameterType="com.jk.dao.PicUploadResult">
		insert into img_item (id,pId,name,url) values (#{id},#{pId},#{name},#{url})
	</insert>
</mapper>